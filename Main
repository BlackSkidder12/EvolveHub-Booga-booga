    -- world buttons
    
    
    KillAura:AddToggle({
        Name = "Auto Kill",
        Default = false,
        Callback = function(Value)
            _G.SkiddedAura = Value
            SkiddedAura()
        end    
    })
    
    Farming:AddToggle({
        Name = "Auto Shark Kill",
        Default = false,
        Callback = function(Value)
            _G.LurkyBoiKill = Value
            LurkyBoiKill()
        end    
    })
    
    Farming:AddToggle({
        Name = "Auto Ancient Tree Kill",
        Default = false,
        Callback = function(Value)
            _G.AncientTreeKill = Value
            AncientTreeKill()
        end    
    })
    
    Farming:AddToggle({
        Name = "Auto Tropical Tree Kill",
        Default = false,
        Callback = function(Value)
            _G.TropicalTreeKill = Value
            TropicalTreeKill()
        end    
