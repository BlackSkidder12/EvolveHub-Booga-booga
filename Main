
    local Library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
    local Window = Library:MakeWindow({Name = "3e44.exe - Booga Hybrid", HidePremium = true, SaveConfig = true, IntroEnabled = false, ConfigFolder = "OrionTest"})
    
    local KillAura = Window:MakeTab({
        Name = "Kill Aura's",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
    
    local Farming = Window:MakeTab({
        Name = "Levels",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
    
    local World = Window:MakeTab({
        Name = "World-Visuals",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
    
    local PlayerEsp = Window:MakeTab({
        Name = "Player-Visuals",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
    
    _G.SkiddedAura = false
    _G.LurkyBoiKill = false
    _G.AncientTreeKill = false
    _G.SmallTropicalTreeKill = false
    _G.WealthyGodKill = false
    _G.GreedyGoddessKill = false
    _G.StrangeMeteorKill = false
    
    -- auto kills
    function SkiddedAura()    
        while _G.SkiddedAura == true and game:GetService("RunService").Heartbeat:Wait() do game:GetService("RunService").RenderStepped:wait() do
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
                
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
            
            
        local function GetClosest()
        local Character = LocalPlayer.Character
        local HumanoidRootPart = Character and Character:FindFirstChild("HumanoidRootPart")
        if not (Character or HumanoidRootPart) then return end
            
        local TargetDistance = math.huge
        local Target
            
        for i,v in ipairs(Players:GetPlayers()) do
        if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
        local TargetHRP = v.Character.HumanoidRootPart
        local mag = (HumanoidRootPart.Position - TargetHRP.Position).magnitude
        if mag < TargetDistance then
        TargetDistance = mag
        Target = v
        end
        end
        end
    
        return tostring(Target)
        end
        local autoatacktarget = GetClosest()
        local args = {
        [1] = {
            ["location"] = nil --[[Vector3]],
            ["weaponName"] = "The Meatmaker",
            ["id"] = 3,
            ["hit"] = game:GetService("Players")[autoatacktarget]
        }
    }
    game:GetService("ReplicatedStorage").Events.Bow.Impact:FireServer(unpack(args))
    end
        end
    end
    
    function LurkyBoiKill()
        while _G.SharkKill == true do
            wait(0.0000001)
            local args = {
                [1] = {
                    ["location"] = nil --[[Vector3]],
                    ["weaponName"] = "The Meatmaker",
                    ["id"] = 3,
                    ["hit"] = workspace.Sharks["Lurky Boi"]
                }
            }
            game:GetService("ReplicatedStorage").Events.Bow.Impact:FireServer(unpack(args))
        end
    end
    
    function AncientTreeKill()
        while _G.AncientTreeKill == true do
            wait(0.0000001)
            local args = {
                [1] = {
                    ["location"] = nil --[[Vector3]],
                    ["weaponName"] = "The Meatmaker",
                    ["id"] = 3,
                    ["hit"] = workspace.Resources["Ancient Tree"]
                }
            }
            game:GetService("ReplicatedStorage").Events.Bow.Impact:FireServer(unpack(args))
        end
    end
    
    
    function TropicalTreeKill()
